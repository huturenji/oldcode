    "buildSandbox": "cross-env BP_ENV=Sandbox BP_APPNAME=voucher node custBuild/build.js",
    "buildProd": "cross-env BP_ENV=Production BP_APPNAME=voucher node custBuild/build.js",
    "dll": "webpack --config build/webpack.dll.config.js -w",
    "fix-memory-limit": "cross-env LIMIT=4096 increase-memory-limit",
    "eslint": "eslint --init",
    "lintfix": "eslint --fix --ext .js,.vue src"
  },
