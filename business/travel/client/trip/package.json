{
  "name": "bp",
  "version": "1.0.0",
  "description": "a vue project",
  "author": "yg",
  "private": true,
  "scripts": {
    "dev": "cross-env BP_ENV=BlackBox webpack-dev-server  --inline --progress --config tripBuild/webpack.dev.conf.js",
    "start": "npm run dev",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "test": "npm run unit",
    "build": "cross-env BP_ENV=Dev BP_APPNAME=trip node tripBuild/build.js",
    "buildBlack": "cross-env BP_ENV=BlackBox BP_APPNAME=trip node tripBuild/build.js",
    "buildSandbox": "cross-env BP_ENV=Sandbox BP_APPNAME=trip node tripBuild/build.js",
    "buildProd": "cross-env BP_ENV=Production BP_APPNAME=trip node tripBuild/build.js",
    "dll": "webpack --config build/webpack.dll.config.js -w",
    "fix-memory-limit": "cross-env LIMIT=4096 increase-memory-limit",
    "eslint": "eslint --init",
    "lint": "eslint src",
    "lintfix": "eslint --fix --ext .js,.vue src"
  },
  "pre-commit": [
    "lintfix"
  ],
  "dependencies": {
