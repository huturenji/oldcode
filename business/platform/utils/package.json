{
  "name": "bp",
  "version": "1.0.0",
  "description": "a vue project",
  "author": "yg",
  "private": true,
  "scripts": {
    "build": "cross-env BP_ENV=Production BP_ENTRY=index webpack --config build/webpack.comp.globaljs.conf.js -w",
